# Environment Variables for Direktori dan Edukasi Komposting Bali
# Copy this file to .env.local for local development
# Set these values in Netlify dashboard for production

# Application Configuration
VITE_APP_TITLE="Direktori dan Edukasi Komposting Bali"
VITE_APP_DESCRIPTION="Platform direktori dan edukasi yang menghubungkan warga Bali dengan layanan komposting lokal"
VITE_APP_URL="https://your-domain.pages.dev"

# Google Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID="G-XXXXXXXXXX"

# reCAPTCHA Configuration (for form spam protection)
VITE_RECAPTCHA_SITE_KEY="6LeXKp0rAAAAAItmfh2ZIhDyBylvWi1KOsafR89j"
RECAPTCHA_SECRET_KEY="your-recaptcha-secret-key"

# Development Settings
NODE_ENV="development"

# Feature Flags
VITE_ENABLE_ANALYTICS="true"
VITE_ENABLE_RECAPTCHA="true"

# Deployment Context (Set automatically by Cloudflare Pages)
# CF_PAGES_URL - Automatically set by Cloudflare Pages
# CF_PAGES_COMMIT_SHA - Git commit SHA
# CF_PAGES_BRANCH - Git branch name


# Supabase Configuration (Required for form submissions)
VITE_SUPABASE_API_KEY="your-supabase-api-key"
VITE_SUPABASE_PROJECT_URL="https://your-supabase-project.supabase.co"
